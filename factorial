import java.util.Scanner;
public class Factorial {
    public static void main (String args[])
    {
        Scanner scanner = new Scanner (System.in);
        int numb = scanner.nextInt();
        int result = 1;

        for (int i = 1; i <= numb; i++)
        {
            result *= i;
        }
        System.out.println(result);
    }
}
